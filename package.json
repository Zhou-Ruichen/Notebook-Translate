{
  "name": "ipynb-markdown-translator",
  "displayName": "Jupyter Notebook Markdown 英译汉",
  "description": "将Jupyter Notebook中的Markdown单元格从英文翻译为中文",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ipynb-translator.translateMarkdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ipynb-translator.translateMarkdown",
        "title": "翻译Notebook Markdown单元格 (英译汉)"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceExtname == .ipynb",
          "command": "ipynb-translator.translateMarkdown",
          "group": "ipynb"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
